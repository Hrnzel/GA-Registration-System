<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="body" prefHeight="500.0" prefWidth="600.0" styleClass="body" stylesheets="@../CSS/Style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddUserController">
   <children>
      <Pane fx:id="topBar" prefHeight="40.0" prefWidth="600.0" styleClass="topBar" stylesheets="@../CSS/Style.css">
         <children>
            <Label fx:id="exit" alignment="CENTER" layoutX="560.0" layoutY="1.0" onMouseClicked="#onExit" prefHeight="39.0" prefWidth="39.0" styleClass="exit" stylesheets="@../CSS/Style.css" text="X" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS Bold" size="18.0" />
               </font>
            </Label>
            <Label fx:id="minimize" alignment="CENTER" layoutX="520.0" layoutY="1.0" onMouseClicked="#minimize" prefHeight="39.0" prefWidth="39.0" styleClass="minimize" stylesheets="@../CSS/Style.css" text="_" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS Bold" size="18.0" />
               </font>
            </Label>
            <Label layoutX="248.0" layoutY="6.0" text="Add User" textFill="WHITE">
               <font>
                  <Font name="Segoe UI Bold" size="24.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <TextField fx:id="txtUsername" layoutX="77.0" layoutY="242.0" prefHeight="40.0" prefWidth="300.0" promptText="Username" styleClass="btnAddUser" stylesheets="@../CSS/Style.css">
         <font>
            <Font name="Arial" size="18.0" />
         </font></TextField>
      <TextField fx:id="txtMiddleName" layoutX="77.0" layoutY="135.0" prefHeight="40.0" prefWidth="300.0" promptText="Middle Name" styleClass="btnAddUser" stylesheets="@../CSS/Style.css">
         <font>
            <Font name="Arial" size="18.0" />
         </font></TextField>
      <TextField fx:id="txtLastName" layoutX="77.0" layoutY="188.0" prefHeight="40.0" prefWidth="300.0" promptText="Last Name" styleClass="btnAddUser" stylesheets="@../CSS/Style.css">
         <font>
            <Font name="Arial" size="18.0" />
         </font></TextField>
      <TextField fx:id="txtFirstName" layoutX="76.0" layoutY="82.0" prefHeight="40.0" prefWidth="300.0" promptText="First Name" styleClass="btnAddUser" stylesheets="@../CSS/Style.css">
         <font>
            <Font name="Arial" size="18.0" />
         </font></TextField>
      <PasswordField fx:id="txtPassword" layoutX="77.0" layoutY="296.0" prefHeight="40.0" prefWidth="300.0" promptText="Password" styleClass="btnAddUser" stylesheets="@../CSS/Style.css">
         <font>
            <Font name="Arial" size="18.0" />
         </font></PasswordField>
      <PasswordField fx:id="txtConfirmPassword" layoutX="77.0" layoutY="350.0" prefHeight="40.0" prefWidth="300.0" promptText="Confirm Password" styleClass="btnAddUser" stylesheets="@../CSS/Style.css">
         <font>
            <Font name="Arial" size="18.0" />
         </font></PasswordField>
      <Button layoutX="226.0" layoutY="433.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" styleClass="btnCam" stylesheets="@../CSS/Style.css" text="Add" textFill="WHITE">
         <font>
            <Font name="Segoe UI Semibold" size="18.0" />
         </font>
      </Button>
   </children>
</Pane>
